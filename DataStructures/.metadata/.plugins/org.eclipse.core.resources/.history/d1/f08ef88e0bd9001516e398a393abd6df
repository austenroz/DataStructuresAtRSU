	public static int findMedian(int inputArray[])
	{
		return findMedian(inputArray, (inputArray.length)/2 + 1, 0, inputArray.length-1);
	}
	
	public static int findMedian(int inputArray[], int k, int low, int high)
	{
		
	}
	
	public static int findPivot(int inputArray[])
	{
		int a = inputArray[0];
		int b = inputArray[(int)((inputArray.length+ 1) / 2)];
		int c = inputArray[inputArray.length-1];
		return max(min(a,b), min(max(a,b),c));
	}
}